@{
    ViewBag.Title = "T74041";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="~/Apps/JSFactory/Transaction/T74041PatFactory.js"></script>
<script src="~/Apps/JSService/Transaction/T74041Service.js"></script>
<script src="~/Apps/JSController/Transaction/T74041Controller.js"></script>
<script src="~/Apps/JSService/LabelService.js"></script>
<style>
    .padrightremove {
        padding-right:0px;
    }
    legend {
        display: block;
        width: auto;
        padding-left: 2px;
        padding-right: 2px;
        border: none;
        margin-bottom: -12px;
    }

    fieldset {
        border: 1px solid #A21217;
        border-radius: 5px;
        padding: 10px 10px 5px 10px;
    }

    .error {
        color: red !important;
    }

    .complete {
        background-color: transparent;
    }

    .green {
        background-color: transparent;
    }

    .cancel {
        background-color:transparent;
    }

    .no-gutter {
        padding-right: 2px;
        padding-left: 2px;
    }

    .no-right-gutter {
        padding-right: 0;
    }

    /*popup css start*/
    .omf-modal {
        display: none;
        position: fixed;
        top: 0;
        /*left: 185px;*/
        width: 100%;
        height: 50vw;
        background-color: rgba(128, 128, 128, 0.00);
        /*overflow: auto;*/
        overflow: hidden !important;
        z-index: 999;
    }

        .omf-modal .omf-modal-body {
            width: 92%;
            padding: 1px;
            margin: auto;
            margin-top: 150px;
            margin-left: 0px;
            background-color: #FFFFFF;
            border-style: ridge;
        }

    nav .nav .btn {
        margin-top: 3px;
    }

    #mainBody {
        margin-top: 20px;
        z-index: 999;
    }

    #bedPopUpTable td {
        border: 1px solid #c2b1b1;
    }

    #PendingPopUpTable td {
        border: 2px solid #c2b1b1;
    }
    #CancelPopUpTable td {
        border: 2px solid #c2b1b1;
    }
    .r-border {
        border-right: 2px solid #ddd;
    }

    .span {
        display: inline-block;
        text-overflow: ellipsis;
        text-align: left;
        vertical-align: middle;
    }

    .tooltip-problem {
        display: inline-block;
        width: 230px;
        white-space: nowrap;
        overflow: hidden !important;
        text-overflow: ellipsis;
        text-align: left;
        float: left;
        margin-left: 8px;
    }

    div.gm-style-iw, .gm-style-iw-d {
        max-height: 300px !important;
        overflow: hidden !important;
    }

    .gm-style-iw .gm-style-iw-c {
        padding-bottom: 5px !important;
      
    }

    .gm-style .gm-style-iw-c {
        background: url(../images/mapTooltip.png);
    }

    /*popup css start*/
    .blue {
        background-color: #D88785 !important;
    }

    .Green {
        background-color: #DEDB98 !important;
    }


    .Button {
        height: 31px !important;
        background-color: transparent !important;
        border: 0px none !important;
        margin-top: 0px !important;
    }

    .custom-label {
        position: relative;
        top: 1px;
        background: url(../images/mapTooltipMini.png);
        background-position: 1px 30px;
        color: #fff;
        padding: 2px;
        line-height: 10px;
        height: 30px;
        border-radius: 5px;
    }

    .blinkSpn {
        animation: blinker 1.6s linear infinite;
        color: #61b851;
    }

    @@keyframes blinker {
        50% {
            opacity: 0;
        }
    }

    .spn {
        display: inline-block;
        width: 40px;
        white-space: nowrap;
        overflow: hidden !important;
        text-overflow: ellipsis;
    }
    
</style>


@section container{
<div id="options" ng-app="Ambucare" ng-controller="T74041Controller">


    <button id="btnSave" tooltip="Save" class="Button Save" title="Save" ng-click="Save()" ng-disabled="obj.myDiv.$invalid"></button>
    <button id="btnClear" tooltip="Clear" class="Button Clear" title="Clear" ng-click="Clear()"></button>
    <button id="btnUnknown" class="Button Unknown" title="Register Unknown Patient" ng-click="unknown()"></button>
    <button id="btnActivePatient" class="Button ActivePatient" title="Active Patient List" ng-click="ActivePatientInfo()"></button>
    <button id="btnPendingRequest" class="Button PendingRequest" title="Pending Request List" ng-click="PatInfo()"></button>
    
</div>
}
<div ng-app="Ambucare" ng-controller="T74041Controller" name="obj.myDiv" novalidate ng-form style="margin-top: 1%;">
    <div style="width: 95%; float: left; margin-left: 2%;">
        <div style="width: 100%">

            <div class="row div_back" style="padding: 5px">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="row" style="padding-top: 0px;">
                        <div style="width: 100%">
                            <div class="head-border" style="margin-bottom: 5px;">

                                <label style="font-size: 1.3em; color: white; margin: 0.2%; margin-left: 0.6%;" id="lblCaseInfo" ng-repeat="field in entity track by $index" class="ng-cloak" ng-if="field.T_LABEL_NAME=='lblCaseInfo'">{{field.T_LANG2_TEXT}}</label>
                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-2" style="width: 12% !important;">
                                
                                <label id="lblCurrLocation" ng-repeat="field in entity track by $index" class="ng-cloak" ng-if="field.T_LABEL_NAME=='lblCurrLocation'">{{field.T_LANG2_TEXT}}</label>
                                <span ng-show="obj.myDiv.Currlocation.$error.required"></span><span class="error">*</span>
                            </div>
                            
                            <div class="col-lg-3 col-md-3 col-sm-3" style="">
                                <input id="gLocation" type="text" gm-places-autocomplete ng-model="obj.autocomplete" style="width: 100%;" required next-focus tabindex="1" />
                                <input id="txtPosLat" type="hidden" ng-model="obj.t74041.T_LATITUDE" ng-bind="obj.t74041.T_LATITUDE=obj.lat" name="Currlocation" required />
                                <input id="txtPosLng" type="hidden" ng-model="obj.t74041.T_LONGITUDE" ng-bind="obj.t74041.T_LONGITUDE=obj.lng" />
                                <input type="hidden" ng-model="obj.type" ng-bind="obj.type" />
                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-2" style="text-align: right;">
                                <img src="~/Images/start.png" ng-click="ShortDistance()" style="width: 22px; height: 22px; border-radius: 5px; cursor: pointer" />
                                <label id="lblTotalDistance" ng-repeat="field in entity track by $index" class="ng-cloak" ng-if="field.T_LABEL_NAME=='lblTotalDistance'"  for="txtEstemetedDistance">{{field.T_LANG2_TEXT}}</label>
                            </div>
                            <div class="col-lg-1 col-md-1 col-sm-1">
                                <input id="txtEstemetedDistance" type="text" style="width: 100%;" ng-model="obj.t74041.T_APPRX_DIST" ng-bind="obj.t74041.T_APPRX_DIST=map.directionsRenderers[0].directions.routes[0].legs[0].distance.text" next-focus tabindex="2" />
                            </div>
                            <div class="col-lg-1 col-md-1 col-sm-1">
                                <label id="lblEstemetedTime" ng-repeat="field in entity track by $index" class="ng-cloak" ng-if="field.T_LABEL_NAME=='lblEstemetedTime'" for="txtEstemetedTime">{{field.T_LANG2_TEXT}}</label>
                            </div>
                            <div class="col-lg-1 col-md-1 col-sm-1">
                                <input id="txtEstemetedTime" type="text" style="width:100%;" ng-model="obj.t74041.T_APPRX_TIME" ng-bind="obj.t74041.T_APPRX_TIME=map.directionsRenderers[0].directions.routes[0].legs[0].duration.text" next-focus tabindex="3" />
                            </div>
                            <div class="col-lg-1 col-md-1 col-sm-1" style="width: 12% !important;">
                                <label id="lblAmbucareTeam" ng-repeat="field in entity track by $index" class="ng-cloak" ng-if="field.T_LABEL_NAME=='lblAmbucareTeam'">{{field.T_LANG2_TEXT}}</label>
                            </div>
                            <div class="col-lg-1 col-md-2 col-sm-2 padrightremove">
                                <ui-select ng-model="obj.k.selected" id="ddlAmbName" theme="select2" style="width: 100%;" append-to-body="true" on-select="onAmbSearch(obj.k.selected.latitude,obj.k.selected.longitude,obj.k.selected.T_AMBU_REG_ID)" tabindex="4">
                                    <ui-select-match placeholder="Select">{{$select.selected.AMB_CAPASITY}}</ui-select-match>
                                    <ui-select-choices repeat="k in places | propsFilter: {AMB_CAPASITY: $select.search}">
                                        <div ng-bind-html="k.AMB_CAPASITY | highlight: $select.search"></div>
                                    </ui-select-choices>
                                </ui-select>
                            </div>
                        </div>

                    </div>
                    <div class="row">
                        <div style="width:100%;">
                            <div class="col-lg-2 col-md-2 col-sm-2" style="width: 12% !important;">
                                <label id="lblProblem" ng-repeat="field in entity track by $index" class="ng-cloak" ng-if="field.T_LABEL_NAME=='lblProblem'" for="txtProblem">{{field.T_LANG2_TEXT}}</label>
                            </div>
                            <div class="col-lg-8 col-md-8 col-sm-8">
                                <textarea id="txtProblem" type="text" maxlength="200" style="max-height: 22px; min-height: 22px; min-width: 100%; max-width: 100%" ng-model="obj.t74041.T_PROBLEM" name="problem" next-focus tabindex="5"></textarea>
                            </div>
                            <div class="col-lg-1 col-md-1 col-sm-1" style="width: 12% !important;">
                                <label style="float: right;" id="lblProblemDuration" ng-repeat="field in entity track by $index" class="ng-cloak" ng-if="field.T_LABEL_NAME=='lblProblemDuration'" for="txtProblemDuration">{{field.T_LANG2_TEXT}}</label>
                            </div>
                            <div class="col-lg-1 col-md-1 col-sm-1 padrightremove">
                                <input id="txtProblemDuration" type="text" style="width:100%;" ng-model="obj.t74041.T_PROBLEM_DURATION" next-focus tabindex="6" />
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row div_back">
                <div class="col-lg-6 no-gutter">
                    <div class="head-border">
                        <label style="font-size: 1.3em; color: white; margin: 0.2%; margin-left: 0.6%;" id="lblCallerInfo" ng-repeat="field in entity track by $index" class="ng-cloak" ng-if="field.T_LABEL_NAME=='lblCallerInfo'">{{field.T_LANG2_TEXT}}</label>
                    </div>
                    <table style="width: 100%;">
                        <tr>
                            <td style="width: 9%;text-align: right; padding-right: 1%;">
                                <label style="float: right;" id="lblCallerFirstName" ng-repeat="field in entity track by $index" class="ng-cloak" ng-if="field.T_LABEL_NAME=='lblCallerFirstName'" for="txtCallerFirstName">{{field.T_LANG2_TEXT}}</label>
                            </td>
                            <td style="width: 10%">
                                <input id="txtCallerFirstName" ng-model="obj.t74120.T_FIRST_LANG2_NAME" type="text" style="width: 100%;" next-focus tabindex="12" />
                            </td>
                            <td style="width: 9%;text-align: right; padding-right: 1%;">
                                <label style="float: right;" id="lblCallerLastName" ng-repeat="field in entity track by $index" class="ng-cloak" ng-if="field.T_LABEL_NAME=='lblCallerLastName'" for="txtCallerLastName">{{field.T_LANG2_TEXT}}</label>
                            </td>
                            <td style="width: 10%">
                                <input id="txtCallerLastName" ng-model="obj.t74120.T_LAST_LANG2_NAME" type="text" style="width: 100%;" next-focus tabindex="13" />
                            </td>
                            
                        </tr>
                        <tr>
                            <td style="width: 6%; text-align: right; padding-right: 1%;">
                                <label style="float: right;" id="lblCallerMobileNo" ng-repeat="field in entity track by $index" class="ng-cloak" ng-if="field.T_LABEL_NAME=='lblCallerMobileNo'" for="txtCallerMobileNo">{{field.T_LANG2_TEXT}}</label>
                            </td>
                            <td style="width: 10%">
                                <input id="txtCallerMobileNo" name="CallerMobileNo" ng-model="obj.t74120.T_MOBILE_NO" type="text" style="width: 100%;" numbers-only next-focus tabindex="14" maxlength="15" />
                            </td>
                            <td style="width: 9%;text-align: right; padding-right: 1%;">
                                <label id="lblReasonofCall" ng-repeat="field in entity track by $index" class="ng-cloak" ng-if="field.T_LABEL_NAME=='lblReasonofCall'" for="txtReasonofCall" style="float: right;">{{field.T_LANG2_TEXT}}</label>
                            </td>
                            <td style="width: 10%">
                                <ui-select ng-model="obj.R.selected" id="ddlCallReason" theme="select2" style="width: 144px; min-width: 144px;" append-to-body="true" next-focus tabindex="15" 
                                           on-select="ddd('txtAddressWhereCalled')">
                                    <ui-select-match placeholder="Select">{{$select.selected.T_REASON_NAME}}</ui-select-match>
                                    <ui-select-choices repeat="R in callReasonList | propsFilter: {T_REASON_NAME: $select.search}">
                                        <div ng-bind-html="R.T_REASON_NAME | highlight: $select.search"></div>
                                    </ui-select-choices>
                                </ui-select>
                            </td>

                        </tr>
                        <tr>
                            <td style="width: 9%;text-align: right;">
                                <label id="lblAddressWhereCalled" ng-repeat="field in entity track by $index" class="ng-cloak" ng-if="field.T_LABEL_NAME=='lblAddressWhereCalled'" for="txtAddressWhereCalled" style="float: right;">{{field.T_LANG2_TEXT}}</label>
                            </td>
                            <td colspan="3" style="width: 10%">
                                <input id="txtAddressWhereCalled" style="width: 100%;" type="text" ng-model="obj.t74120.T_ADDRESS" next-focus tabindex="16" />
                            </td>
                        </tr>
                    </table>
                </div>
                <div class="col-lg-6 no-gutter">
                    <div class="head-border">

                        <label style="font-size: 1.3em; color: white; margin: 0.2%; margin-left: 0.6%;" id="lblPatientInfo" ng-repeat="field in entity track by $index" class="ng-cloak" ng-if="field.T_LABEL_NAME=='lblPatientInfo'">{{field.T_LANG2_TEXT}}</label>
                    </div>

                    <table style="width: 100%;">
                        <tr>
                            <td style="width:22%; text-align: right; padding-right: 1%;">
                                <label style="float: right;" id="lblFirstName" ng-repeat="field in entity track by $index" class="ng-cloak" ng-if="field.T_LABEL_NAME=='lblFirstName'" for="txtFirstName">{{field.T_LANG2_TEXT}}</label>
                            </td>
                            <td style="width: 35%">
                                <input id="txtFirstName" name="fullname" ng-model="obj.t74046.T_FIRST_LANG2_NAME" type="text" style="width: 100%;" ng-dblclick="PatInfo()" next-focus tabindex="17" />
                                <input id="txtPatId" ng-model="obj.t74041.T_PAT_ID" type="hidden" style="width: 140px;" />
                            </td>
                            <td style="width:22%; text-align: right;padding-right: 1%">
                                <label style="float: right;" id="lblLastName" ng-repeat="field in entity track by $index" class="ng-cloak" ng-if="field.T_LABEL_NAME=='lblLastName'" for="txtLastName">{{field.T_LANG2_TEXT}}</label>
                            </td>
                            <td style="width: 35%">
                                <input id="txtLastName" ng-model="obj.t74046.T_GFATHER_LANG2_NAME" type="text" style="width: 100%;" next-focus tabindex="18" />
                            </td>
                        </tr>
                        <tr>
                            <td style="width:22%; text-align: right; padding-right: 1%;">
                                <label id="lblGender" ng-repeat="field in entity track by $index" class="ng-cloak" ng-if="field.T_LABEL_NAME=='lblGender'" for="ddlGenName" style="float: right;">{{field.T_LANG2_TEXT}}</label>
                            </td>
                            <td style="width:20%">
                                <ui-select ng-model="obj.G.selected" id="ddlGenName" theme="select2" style="min-width: 144px; width: 144px;" append-to-body="true" next-focus tabindex="19" on-select="ddd('txtAge')">
                                    <ui-select-match placeholder="Select">{{$select.selected.T_LANG2_NAME}}</ui-select-match>
                                    <ui-select-choices repeat="G in gender | propsFilter: {T_LANG2_NAME: $select.search}">
                                        <div ng-bind-html="G.T_LANG2_NAME | highlight: $select.search"></div>
                                    </ui-select-choices>
                                </ui-select>
                                <input type="hidden" id="ddlGenId" ng-model="obj.t74046.T_SEX_CODE" ng-bind="obj.t74046.T_SEX_CODE = obj.G.selected.T_SEX_CODE" name="gender" style="width: 124px" />
                            </td>

                            <td style="width:22%; text-align: right; padding-right: 1%;">
                                <label style="float: right;" id="lblAge" ng-repeat="field in entity track by $index" class="ng-cloak" ng-if="field.T_LABEL_NAME=='lblAge'" for="txtAge">{{field.T_LANG2_TEXT}}</label>
                            </td>
                            <td style="width:20%;">
                                <input id="txtAge" type="text" style="width: 54%;" ng-keydown="ckAge(obj.t74041.T_AGE)" ng-model="obj.t74041.T_AGE" name="ageLevel" numbers-only next-focus tabindex="20" maxlength="3" />&nbsp;<span>Yrs</span>
                            </td>



                        </tr>
                        <tr>
                            
                            <td style="width:22%; text-align: right;">
                                <label id="lblContactNo" ng-repeat="field in entity track by $index" class="ng-cloak" ng-if="field.T_LABEL_NAME=='lblContactNo'" for="txtContactNo">{{field.T_LANG2_TEXT}}</label>
                                <span id="spnClass" class="error"></span>
                            </td>
                            <td style="width:35%">
                                <input id="txtContactNo" type="text" style="width: 145px;" ng-model="obj.t74046.T_MOBILE_NO" name="contactNo" numbers-only next-focus tabindex="21" maxlength="15" />
                            </td>
                            <td style="width:22%; text-align: right; padding-right:1%;">
                                <label id="lblPatNationalId" ng-repeat="field in entity track by $index" class="ng-cloak" ng-if="field.T_LABEL_NAME=='lblPatNationalId'" for="txtPat_NationalId" style="float: right;">{{field.T_LANG2_TEXT}}</label>
                            </td>
                            <td style="width: 35%" colspan="4">
                                <input id="txtPat_NationalId" type="text" ng-model="obj.t74046.T_NATIONAL_ID" style="width: 145px;" numbers-only next-focus tabindex="22" maxlength="15" numbers-only />
                            </td>
                        </tr>
                       
                    </table>
                </div>
            </div>



        </div>

    </div>
    <div style="float: left; width: 95%; margin-top: 0.3%; margin-left: 2%">
        <div class="row div_back" style="padding: 5px">
            <table style="margin: auto; width: 98%">
                <tr>
                    <td style="width: 2%; text-align: center">
                        <input type="radio" id="rad01" value="1" ng-model="obj.selectValue" ng-click="LoadDropDownList(obj.selectValue,1)" style="margin-bottom: 10px;" />
                    </td>
                    <td style="width: 10%; text-align: left">
                        <label for="rad01" ng-repeat="field in entity track by $index" class="ng-cloak" ng-if="field.T_LABEL_NAME=='lblAbleAbu'">{{field.T_LANG2_TEXT}}</label>
                    </td>

                    <td style="width: 2%; text-align: center">
                        <input type="radio" id="rad02" value="2" ng-model="obj.selectValue" ng-click="LoadDropDownList(obj.selectValue,1)" style="margin-bottom: 10px;" />
                    </td>
                    <td style="width: 10%; text-align: left">
                        <label for="rad02" ng-repeat="field in entity track by $index" class="ng-cloak" ng-if="field.T_LABEL_NAME=='lblAmuPat'">{{field.T_LANG2_TEXT}}</label>
                    </td>

                    <td style="width: 2%; text-align: center">
                        <input type="radio" id="rad03" value="3" ng-model="obj.selectValue" ng-click="LoadDropDownList(obj.selectValue,1)" style="margin-bottom: 10px;" />
                    </td>
                    <td style="width: 10%; text-align: left">
                        <label for="rad03" ng-repeat="field in entity track by $index" class="ng-cloak" ng-if="field.T_LABEL_NAME=='lblAmuCln'">{{field.T_LANG2_TEXT}}</label>
                    </td>


                    <td style="width: 2%; text-align: center">
                        <input type="radio" id="rad04" value="4" ng-model="obj.selectValue" ng-click="LoadDropDownList(obj.selectValue,1)" style="margin-bottom: 10px;" />
                    </td>
                    <td style="width: 10%; text-align: left">
                        <label for="rad04" ng-repeat="field in entity track by $index" class="ng-cloak" ng-if="field.T_LABEL_NAME=='lblNotAble'">{{field.T_LANG2_TEXT}}</label>
                    </td>
                   
                    <td style="width: 2%;vertical-align: initial;background: url(../Images/activePatientNo.png) no-repeat;">
                        <span class="label label-default complete" style="width: 5px;color: #000 !important;font-size: 10px;margin: 7px;padding: 0;">{{activeLen}}</span>
                        @*<input type="button" class="btn btn-default"/>*@
                    </td>
                    <td style="width: 10%; text-align: left">
                        <label style="margin-bottom: 0px !important;" ng-repeat="field in entity track by $index" class="ng-cloak" ng-if="field.T_LABEL_NAME=='lblAcPat'">{{field.T_LANG2_TEXT}}</label>
                    </td>
                    
                    <td style="width: 2%;vertical-align: initial;background: url(../Images/pendingPatientNo.png) no-repeat;">
                        <span class="label label-primary green" style="width: 5px;color: #000 !important;font-size: 10px;margin: 7px;padding: 0;">{{pendingLen}}</span>
                    </td>
                    <td style="width: 10%; text-align: left">
                        <label style="margin-bottom: 0px !important;" ng-repeat="field in entity track by $index" class="ng-cloak" ng-if="field.T_LABEL_NAME=='lblPendingReqLst'" title="{{field.T_LANG2_TEXT}}"><span id="blk">{{field.T_LANG2_TEXT}}</span></label>


                    </td>
                    <td style="width: 2%; text-align: center;vertical-align: initial">
                        <div>
                            <sound-button data-ng-click="play()" origem="~/audio/Industrial%20Alarm-SoundBible.com-1012301296.mp3">
                                <span style="display: block; color: #fff; padding-top: 10px; margin: 1px;cursor:pointer"><img title="Stop" src="~/Images/CloseChat1.png" /></span>
                            </sound-button>
                        </div>
                    </td>
                    <td style="display: none;width: 2%;vertical-align: initial;background: url(../Images/cancelRequestNo.png) no-repeat;">
                        <span class="label label-primary cancel" style="width: 5px;color: #000 !important;font-size: 10px;margin: 7px;padding: 0;">{{CancelCount}}</span>
                    </td>
                    <td style="display: none;width: 13%; text-align: right">
                        <label style="margin-bottom: 0px !important;" ng-repeat="field in entity track by $index" class="ng-cloak" ng-if="field.T_LABEL_NAME=='lblCancelRequestList'">{{field.T_LANG2_TEXT}}</label>
                    </td>
                </tr>
            </table>
        </div>

    </div>
    <div style="float: left; width: 95%; margin-top: 0.3%; margin-left: 2%;">
        <div class="row">
            <div style="border: 1px solid #adadad; border-radius: 5px;">

                <ng-map id="gmap" center="{{cx.latitude}}, {{cx.longitude}}" zoom="12" style="height: 47vh; width: 100%">
                    <traffic-layer></traffic-layer>
                    <info-window id="myInfoWindow">
                        <div ng-non-bindable class="ng-cloak">
                            <table style="width: 450px;color:#fff">
                                <tr style="display: table;">
                                    <td style="width: 130px">
                                        <b>
                                            <label id="lblAmId" ng-repeat="field in entity track by $index" class="ng-cloak" ng-if="field.T_LABEL_NAME=='lblAmId'">{{field.T_LANG2_TEXT}}</label>
