
@{
    ViewBag.Title = "T74015";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<link href="~/Content/jquery.dataTables.min.css" rel="stylesheet" />
<script src="../../Apps/JSController/Transaction/T74015Controller.js"></script>
<script src="../../Apps/JSService/Transaction/T74015Service.js"></script>
<script src="../../Apps/JSService/LabelService.js"></script>
<script src="../../Apps/JSFactory/Transaction/T74015/EmployeeFactory.js"></script>
<script src="../../Apps/JSFactory/Transaction/T74015/EmployeeTypeFactory.js"></script>
@section container{
    <div id="options" ng-app="Ambucare" ng-controller="T74015Controller">
        <button ID="btnSave" Tooltip="Save" class="Button Save" ng-click="Save_Click()"></button>
        <button ID="btnDelete" ToolTip="Delete" class="Button Delete" ng-click="Delete_Click()"></button>
        <button ID="btnClear" ToolTip="Clear" class="Button Clear" ng-click="Clear()"></button>
        @*<button ID="btnPrint" ToolTip="Print" class="Button Print" ng-click="Print()"></button>*@
    </div>
}

<div class="container" ng-app="Ambucare" ng-controller="T74015Controller">
    <div class="row" style="padding-bottom: 8px">
        <!--working div-->
        <div class="col-lg-12 col-md-12 col-sm-12">
            <div class="col-lg-3 col-md-3 col-sm-3"></div>
            <div id="DivMaritalStatus" class="tab-content col-lg-6 col-md-6 col-sm-6" style="margin: 1.6% auto; border: 1px solid #A21217; border-radius: 5px; padding: 10px">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-1 col-md-1 col-sm-1"></div>
                        <div class="col-lg-4 col-md-4 col-sm-4">
                            <label id="lblAmbCapaCode" ng-repeat="field in entity track by $index" class="ng-cloak" ng-if="field.T_LABEL_NAME=='lblAmbCapaCode'" for="ddlAmbCapaCode">{{field.T_LANG2_TEXT}}</label>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6">
                            <div class="ng-cloak select-box">
                                <ui-select ng-model="obj.A.selected" on-select="check()" id="ddlAmbCapaCode" theme="select2" append-to-body="true" style="min-width: 245px;max-width: 300px">
                                    <ui-select-match placeholder="Select">{{$select.selected.T_SERIES}}</ui-select-match>
                                    <ui-select-choices repeat="A in ambulance | propsFilter: {T_SERIES: $select.search}">
                                        <div ng-bind-html="A.T_SERIES | highlight: $select.search"></div>
                                    </ui-select-choices>
                                </ui-select>
                                <input type="text" id="ddlIAmbCapaCode" ng-model="obj.T_AMBU_REG_ID" ng-bind="obj.T_AMBU_REG_ID=obj.A.selected.T_AMBU_REG_ID" class="Hidden" />
                            </div>
                        </div>
                        <div class="col-lg-1 col-md-1 col-sm-1"></div>
                    </div>
                </div>
            </div>
            <div class="col-lg-3 col-md-3 col-sm-3"></div>
        </div>
    </div>


    <div class="row">
        <div>
            <div style="margin: 10px auto; border: 1px solid #A21217; border-radius: 5px; width: 900px;">
                <center>
                    <table class="table table-bordered ng-cloak" style="margin:auto">
                        <thead style="text-align: center">
                            <tr bgcolor="#D8454B" style="color: #ffffff">
                                <!--<th>Add</th>-->
                                <th style="text-align: center"><input type="button" style="background: url(../Images/Plus_white.png); background-size: 15px; background-repeat: no-repeat; background-position: center; width: 15px; height: 15px; float: right;margin:5px 25px 0 0; align-content: right" ng-click="Addrow()" /></th>
                                <th>Index</th>
                                <th>Employee Type Id</th>
                                <th>Employee Type Name</th>
                                <th>Employee Id</th>
                                <th>Employee Name</th>

                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="obj in obj.employeeInfo">
                                <!-- <td>
                                    <input type="button" class="AddButton" style="background: url(../Images/plus.png); background-size: 30px; background-repeat: no-repeat; background-position: center; width: 30px;" ng-click="Addrow(items)"/>
                                </td>-->
                                <td style="text-align: center">
                                    <input type="button" style="background: url(../Images/Reject.jpg); background-size: 20px; background-repeat: no-repeat; background-position: center; width: 20px;" ng-click="RemoveRow($index)" />
                                </td>
                                <td style="text-align: center">{{$index+1}}</td>
                                <td>
                                    <input type="text" id="txtEmployeeTypeId{{$index}}" name="T_EMP_TYP_ID" class="form-control" readonly="readonly" style="width: 100%" ng-dblclick="empType($index)" ng-model="obj.T_EMP_TYP_ID">
                                </td>
                                <td>
                                    <input type="text" id="txtEmployeeTypeName{{$index}}" name="T_EMP_TYP_NAME" class="form-control" readonly="readonly" style="width: 100%" ng-model="obj.T_EMP_TYP_NAME">
                                </td>
                                <td>
                                    <input type="text" id="txtEmployeeId{{$index}}" name="T_EMP_ID" class="form-control" readonly="readonly" style="width: 100%" ng-dblclick="emp($index)" ng-model="obj.T_EMP_ID">
                                </td>
                                <td>
                                    <input type="text" id="txtEmployeeName{{$index}}" name="FullName" class="form-control" readonly="readonly" style="width: 100%" ng-model="obj.FullName">
                                </td>
                                <td style="visibility: hidden">
                                    <input type="hidden" id="txtT_EMP_ASSIGN_ID" class="hidden" ng-model="obj.T_EMP_ASSIGN_ID" />
                                </td>

                            </tr>
                        </tbody>
                    </table>
                </center>
            </div>
        </div>
    </div>
</div>

