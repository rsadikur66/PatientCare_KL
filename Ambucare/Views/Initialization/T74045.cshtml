
@{
    ViewBag.Title = "T74045";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<link href="~/Content/jquery.dataTables.min.css" rel="stylesheet" />
<script src="~/Apps/JSService/LabelService.js"></script>
<script src="~/Apps/JSController/Initialization/T74045Controller.js"></script>
<script src="~/Apps/JSService/Initialization/T74045Service.js"></script>

@section container{
    <div id="options" ng-app="Ambucare" ng-controller="T74045Controller">
        <button ID="btnSave" Tooltip="Save" class="Button Save" ng-click="Save()"></button>
        <button ID="btnDelete" ToolTip="Delete" class="Button Delete" ng-click="Delete()"></button>
        <button ID="btnClear" ToolTip="Clear" class="Button Clear" ng-click="Clear()"></button>
    </div>
}

<div id="container" ng-app="Ambucare" ng-controller="T74045Controller">
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <div class="col-lg-1 col-md-1 col-sm-1"></div>
            <div class="col-lg-10 col-md-10 col-sm-10" style="text-align: center;">
                <div class="col-lg-12 col-md-12 col-sm-12" style="margin: 1.6% auto; border: 1px solid #A21217; border-radius: 5px; padding: 8px">
                    <div class="row text-center">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                <label id="lblSupplierId" for="txtSupplierId" ng-repeat="field in entity track by $index" ng-if="field.T_LABEL_NAME=='lblSupplierId'" class="ng-cloak" style="float: right;">{{field.T_LANG2_TEXT}}</label>
                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                <input id="txtSupplierId" type="text" readonly="readonly" ng-model="obj.T_SUPPLIER_ID" class="form-control validate[required]" style="width: 50%;float: left;" />
                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                <label id="lblEnglishName" ng-repeat="field in entity track by $index" ng-if="field.T_LABEL_NAME=='lblEnglishName'" class="ng-cloak" Text="" for="txtEnglishName" style="float: right;">{{field.T_LANG2_TEXT}}</label>
                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                <input id="txtEnglishName" type="text" ng-model="obj.T_LANG2_NAME" class="form-control validate[required]" />
                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                <label id="lblLocalName" ng-repeat="field in entity track by $index" ng-if="field.T_LABEL_NAME=='lblLocalName'" class="ng-cloak" Text="" for="txtLocalName" style="float: right;">{{field.T_LANG2_TEXT}}</label>
                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                <input id="txtLocalName" type="text" ng-model="obj.T_LANG1_NAME" class="form-control" />
                            </div>
                        </div>
                    </div>
                    <div class="row text-center" style="padding-bottom: 8px; padding-top: 8px">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                <label id="lblPresentAddress" for="txtPresentAddress" ng-repeat="field in entity track by $index" ng-if="field.T_LABEL_NAME=='lblPresentAddress'"  class="ng-cloak" style="float: right;">{{field.T_LANG2_TEXT}}</label>
                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                <input id="txtPresentAddress" type="text" ng-model="obj.T_PRES_ADDRESS" class="form-control validate[required]" style="width: 70%;float: left;" />
                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                <label id="lblPermaAddress" ng-repeat="field in entity track by $index" ng-if="field.T_LABEL_NAME=='lblPermaAddress'" class="ng-cloak" Text="" for="txtPermaAddress" style="float: right;">{{field.T_LANG2_TEXT}}</label>
                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                <input id="txtPermaAddress" type="text" ng-model="obj.T_PER_ADDRESS" class="form-control validate[required]" />
                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                <label id="lblPhoneNo" ng-repeat="field in entity track by $index" ng-if="field.T_LABEL_NAME=='lblPhoneNo'" class="ng-cloak" Text="" for="txtPhoneNo" style="float: right;">{{field.T_LANG2_TEXT}}</label>
                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                <input id="txtPhoneNo" type="text" ng-model="obj.T_PHONE_NUMBER" class="form-control validate[required]" />
                            </div>
                        </div>
                    </div>
                    <div class="row text-center" style="padding-bottom: 8px; padding-top: 8px">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                <label id="lblMobileNo" for="txtMobileNo" ng-repeat="field in entity track by $index" ng-if="field.T_LABEL_NAME=='lblMobileNo'" class="ng-cloak" style="float: right;">{{field.T_LANG2_TEXT}}</label>
                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                <input id="txtMobileNo" type="text" ng-model="obj.T_MOBILE_NUMBER" class="form-control validate[required]" style="width: 70%;float: left;" />
                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                <label id="lblEmail" ng-repeat="field in entity track by $index" ng-if="field.T_LABEL_NAME=='lblEmail'" class="ng-cloak" Text="" for="txtEmail" style="float: right;">{{field.T_LANG2_TEXT}}</label>
                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                <input id="txtEmail" type="text" ng-model="obj.T_EMAIL_ID" class="form-control validate[required]" />
                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                
                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-2">
                               
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-1 col-md-1 col-sm-1"></div>
        </div>
    </div>
    <!--For Data Gridview start-->
    <div class="row">
        <div class="col-lg-1 col-md-1 col-sm-1"></div>
        <div class="col-lg-10 col-md-10 col-sm-10">
            <center>
                <table>
                    <tr>
                        <td>
                            <div class="table-responsive">
                                <table id="entry-grid" datatable="" dt-options="dtOptions" dt-columns="dtColumns" class="table table-hover" style="width: 100%;"></table>
                            </div>
                        </td>
                    </tr>
                </table>
            </center>
        </div>
        <div class="col-lg-1 col-md-1 col-sm-1"></div>
    </div>
    <!--For Data Gridview End-->
</div>
