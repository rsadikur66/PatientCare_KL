
@{
    ViewBag.Title = "T74024";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="~/Apps/JSController/Initialization/T74024Controller.js"></script>
<script src="~/Apps/JSService/Initialization/T74024Service.js"></script>
<script src="~/Apps/JSService/LabelService.js"></script>
<link href="~/Content/jquery.dataTables.min.css" rel="stylesheet" /> 
@section container{
    <div id="options" ng-app="Ambucare" ng-controller="T74024Controller">
        <button ID="btnSave" Tooltip="Save" class="Button Save" ng-click="Save()"></button>
        <button ID="btnDelete" ToolTip="Delete" class="Button Delete" ng-click="Delete()"></button>
        <button ID="btnClear" ToolTip="Clear" class="Button Clear" ng-click="Clear()"></button>
    </div>
}
<div class="container" ng-app="Ambucare" ng-controller="T74024Controller">
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <div></div>
            <div class="col-lg-4 col-md-4 col-sm-4"></div>
            <div class="col-lg-4 col-md-4 col-sm-4" style="margin: 1.6% auto; border: 1px solid #e17d7d; border-radius: 5px; padding: 10px">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        @*<div class="col-lg-5 col-md-6 col-sm-6">
                            <label id="lblT_RESULT_TYP_ID" for="txtT_RESULT_TYP_ID">Result Type Id</label>
                        </div>*@
                        <div class="col-lg-7 col-md-6 col-sm-6">
                            <input id="txtT_RESULT_TYP_ID" type="hidden" ng-model="obj.T_RESULT_TYP_ID" class="form-control" style="width: 45%" readonly="readonly" />
                        </div>

                    </div>
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-5 col-md-6 col-sm-6">
                            <label id="lblEnglishName"  ng-repeat="field in entity track by $index" ng-if="field.T_LABEL_NAME=='lblEnglishName'" class="ng-cloak" Text="">{{field.T_LANG2_TEXT}}</label>
                        </div>
                        <div class="col-lg-7 col-md-6 col-sm-6">
                            <input id="txtEnglishName" type="text" ng-model="obj.T_LANG2_NAME" class="form-control" />
                        </div>
                        <div>
                        </div>
                    </div>
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-5 col-md-6 col-sm-6">
                            <label id="lblLocalName" associatedcontrolid="txtLocalName"ng-repeat="field in entity track by $index" ng-if="field.T_LABEL_NAME=='lblLocalName'" class="ng-cloak" Text="">{{field.T_LANG2_TEXT}}</label>
                        </div>
                        <div class="col-lg-7 col-md-6 col-sm-6">
                            <input id="txtLocalName" type="text" ng-model="obj.T_LANG1_NAME" class="form-control" />
                        </div>
                    </div>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-4"></div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-1 col-md-1 col-sm-1"></div>
        <div class="col-lg-10 col-md-10 col-sm-10">
            <center>

                <table>
                    <tr>
                        <td>
                            <div class="table-responsive">
                                <table id="entry-grid" width="600px" datatable="" dt-options="dtOptions" dt-columns="dtColumns" class="table table-hover" style="width: 100%;"></table>
                            </div>
                        </td>
                    </tr>
                </table>
            </center>
        </div>
        <div class="col-lg-1 col-md-1 col-sm-1"></div>
    </div>
</div>
