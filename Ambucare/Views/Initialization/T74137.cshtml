<style>
    /*Toggling Div---------------------------Start*/
    .ultab {
        list-style: none;
        padding: 0;
        margin-left: 32%;
    }

    .litab {
        float: left;
        border-bottom-width: 0;
        margin: 3px 3px 3px 3px;
        padding: 8px 5px 25px 5px;
        background-color: #239e6b80;
        color: #FFFFFF;
        border-radius: 3px;
        transition: 0.2s;
        margin-right: -2px;
        width: 150px;
        text-align: center;
        height: 0px;
    }

    #mainView {
        clear: both;
        padding: 0 1em;
    }

    .active {
        background-color: #CCCCCC;
        color: #000000;
    }
    /*Toggling Div---------------------------End*/
</style>


@section container{
    <div id="options" ng-app="Ambucare" ng-controller="T74137Controller">
        <button ID="btnSave" Tooltip="Save" class="Button Save" ng-click="Save_Click()" ng-disabled="obj.myDiv.$invalid"></button>
        @*<button ID="btnDelete" ToolTip="Delete" class="Button Delete" ng-click="Delete()"></button>*@
        <button ID="btnClear" ToolTip="Clear" class="Button Clear" ng-click="Clear()"></button>
    </div>
}


<div class="container" ng-app="Ambucare" ng-controller="T74137Controller">
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <!-- for tabs -->
            <input id="txtHiddenField" type="hidden" style="width: 50px" ng-model="obj.hiddenField" />
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div id="tabs" style="text-align: center">
                    <ul class="ultab">
                        <li class="litab" ng-repeat="tab in tabs" style="cursor: pointer"
                            ng-class="{active:isActiveTab(tab.url)}"
                            ng-click="onClickTab(tab)">{{tab.title}}</li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div class="row text-center">
        <!-- For Input Table -->
        <div class="col-lg-12 col-md-12 col-sm-12">
            @*<div class="col-lg-1 col-md-1 col-sm-1"></div>*@
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div id="mainView">
                    <div ng-include="currentTab"></div>
                </div>
            </div>
            @*<div class="col-lg-1 col-md-1 col-sm-1"></div>*@
        </div>
    </div>
</div>

