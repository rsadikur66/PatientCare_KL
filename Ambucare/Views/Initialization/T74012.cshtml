
@{
    ViewBag.Title = "T74012";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Content/jquery.dataTables.min.css" rel="stylesheet" />
<script src="../../Apps/JSService/Initialization/T74012Service.js"></script>
<script src="~/Apps/JSService/LabelService.js"></script>
<script src="../../Apps/JSController/Initialization/T74012Controller.js"></script>

@section container{
    <div id="options" ng-app="Ambucare" ng-controller="T74012Controller">
        <button ID="btnSave" Tooltip="Save" class="Button Save" ng-click="Save()"></button>
        <button ID="btnDelete" ToolTip="Delete" class="Button Delete" ng-click="Delete()"></button>
        <button ID="btnClear" ToolTip="Clear" class="Button Clear" ng-click="Clear()"></button>
    </div>
}
<div class="container" ng-app="Ambucare" ng-controller="T74012Controller">
    <div class="row" style="padding-bottom: 8px">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <div class="col-lg-3 col-md-3 col-sm-3"></div>
            <div class="col-lg-6 col-md-6 col-sm-6" style="margin: 1.6% auto; border: 2px solid #A21217; border-radius: 5px; padding: 10px">
              <div class="row text-center" style="padding-bottom: 8px; padding-top: 8px">
                    <div class="col-lg-12 col-md-12 col-sm-12 form-inline">
                        <div class="col-lg-4 col-md-4 col-sm-4 form-inline form-group">
                            <label id="lblDetailId" ng-repeat="field in entity track by $index" ng-if="field.T_LABEL_NAME=='lblDetailId'" class="ng-cloak" AssociatedControlID="txtDetailId">{{field.T_LANG2_TEXT}}</label>
                        </div>
                        <div class="col-lg-8 col-md-8 col-sm-8 form-inline form-group">
                            <input id="txtDetailId" type="text" ng-model="obj.T_DETAIL_ID" style="width: 50%; height: 25px; text-align: left; display: inherit;" readonly="readonly" />
                        </div>
                    </div>
                </div>
                <div class="row text-center" style="padding-bottom: 8px">
                    <div class="col-lg-12 col-md-12 col-sm-12 form-inline">
                        <div class="col-lg-4 col-md-4 col-sm-4 form-inline form-group">
                            <label id="lblRequestId" ng-repeat="field in entity track by $index" ng-if="field.T_LABEL_NAME=='lblRequestId'" class="ng-cloak" AssociatedControlID="txtRequestId">{{field.T_LANG2_TEXT}}</label>
                        </div>
                        <div class="col-lg-8 col-md-8 col-sm-8 form-inline form-group">
                            <input id="txtRequestId" type="text" ng-model="obj.T_REQUEST_ID" style="width: 50%; height: 25px;text-align: left;display: inherit;" />
                        </div>
                    </div>
                </div>
                <div class="row text-center" style="padding-bottom: 8px">
                    <div class="col-lg-12 col-md-12 col-sm-12 form-inline">
                        <div class="col-lg-4 col-md-4 col-sm-4 form-inline form-group">
                            <label id="lblUserId" ng-repeat="field in entity track by $index" ng-if="field.T_LABEL_NAME=='lblUserId'" class="ng-cloak" AssociatedControlID="txtUserId">{{field.T_LANG2_TEXT}}</label>
                        </div>
                        <div class="col-lg-8 col-md-8 col-sm-8">
                            <input id="txtUserId" type="text" ng-model="obj.T_USER_ID" style="width: 50%; height: 25px; text-align: left;display: inherit;" />
                        </div>
                    </div>
                </div>
                <div class="row text-center" style="padding-bottom: 8px">
                    <div class="col-lg-12 col-md-12 col-sm-12 form-inline">
                        <div class="col-lg-4 col-md-4 col-sm-4 form-inline form-group">
                            <label id="lblSiteNo" ng-repeat="field in entity track by $index" ng-if="field.T_LABEL_NAME=='lblSiteNo'" class="ng-cloak" AssociatedControlID="txtSiteNo">{{field.T_LANG2_TEXT}}</label>
                        </div>
                        <div class="col-lg-8 col-md-8 col-sm-8 form-inline form-group">
                            <input id="txtSiteNo" type="text" ng-model="obj.T_SITE_NO" style="width: 50%; height: 25px; text-align: left;display: inherit;" />
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-3 col-md-3 col-sm-3"></div>
        </div>
    </div>
    <!--For Data Gridview start-->
    <div class="row">
        <div class="col-lg-1 col-md-1 col-sm-1"></div>
        <div class="col-lg-10 col-md-10 col-sm-10">
            <center>
                <table>
                    <tr>
                        <td>
                            <div class="table-responsive">
                                <table  id="entry-grid" datatable="" dt-options="dtOptions" dt-columns="dtColumns" class="table table-hover" style="width: 100%;"></table>
                            </div>
                        </td>
                    </tr>
                </table>
            </center>
        </div>
        <div class="col-lg-1 col-md-1 col-sm-1"></div>
    </div>
    <!--For Data Gridview End-->
</div>