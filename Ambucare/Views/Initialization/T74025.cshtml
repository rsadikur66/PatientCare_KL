
@{
    ViewBag.Title = "T74025";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/Apps/JSController/Initialization/T74025Controller.js"></script>
<script src="~/Apps/JSService/Initialization/T74025Service.js"></script>
<script src="~/Apps/JSService/LabelService.js"></script>
@section container{
    <div id="options" ng-app="Ambucare" ng-controller="T74025Controller">
        <button ID="btnSave" Tooltip="Save" class="Button Save" ng-click="Save()"></button>
        <button ID="btnDelete" ToolTip="Delete" class="Button Delete" ng-click="Delete()"></button>
        <button ID="btnClear" ToolTip="Clear" class="Button Clear" ng-click="Clear()"></button>


    </div>
}
<div class="container" ng-app="Ambucare" ng-controller="T74025Controller">
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <div></div>
            <div class="col-lg-4 col-md-4 col-sm-4"></div>
            <div class="col-lg-4 col-md-4 col-sm-4" style="margin: 1.6% auto; border: 1px solid #e17d7d; border-radius: 5px; padding: 10px">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12">

                        <div class="col-lg-5 col-md-6 col-sm-6">
                            <label id="lblUserTypeId" class="ng-cloak" ng-repeat="field in entity track by $index" ng-if="field.T_LABEL_NAME=='lblUserTypeId'" class="ng-cloak" for="txtUserTypeId">{{field.T_LANG2_TEXT}} </label>
                        </div>
                        <div class="col-lg-7 col-md-6 col-sm-6">
                            <input id="txtUserTypeId" type="text" ng-model="obj.T_USER_TYPE_ID" class="form-control" style="width: 45%" readonly="readonly" />
                        </div>

                    </div>
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-5 col-md-6 col-sm-6">
                            <label id="lblT_TYPE_NAME1" class="ng-cloak" ng-repeat="field in entity track by $index" ng-if="field.T_LABEL_NAME=='lblT_TYPE_NAME1'" class="ng-cloak" for="txtT_TYPE_NAME1">{{field.T_LANG2_TEXT}} </label>
                        </div>
                        <div class="col-lg-7 col-md-6 col-sm-6">
                            <input id="txtT_TYPE_NAME1" type="text" ng-model="obj.T_TYPE_NAME1" />
                        </div>
                        <div>
                        </div>
                    </div>
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="col-lg-5 col-md-6 col-sm-6">
                            <label id="lblT_TYPE_NAME2" class="ng-cloak" ng-repeat="field in entity track by $index" ng-if="field.T_LABEL_NAME=='lblT_TYPE_NAME2'" class="ng-cloak" for="txtT_TYPE_NAME2">{{field.T_LANG2_TEXT}} </label>
                        </div>
                        <div class="col-lg-7 col-md-6 col-sm-6">
                            <input id="txtT_TYPE_NAME2" type="text" ng-model="obj.T_TYPE_NAME2" class="form-control" />
                        </div>
                    </div>
                </div>

                <div class="col-lg-4 col-md-4 col-sm-4"></div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-2 col-md-2 col-sm-2"></div>
        <div class="col-lg-8 col-md-8 col-sm-8">
            <center>

                <table>
                    <tr>
                        <td>
                            <div class="table-responsive">
                                <table id="entry-grid" datatable="" dt-options="dtOptions" dt-columns="dtColumns" class="table table-hover"></table>
                            </div>
                        </td>
                    </tr>
                </table>
            </center>
        </div>
        <div class="col-lg-2 col-md-2 col-sm-2"></div>
    </div>
</div>
