    <button ID="btnQuery" class="Button Query ng-cloak" ng-click="Menu(4)" ng-repeat="field in Menuentity track by $index" ng-if="field.T_LABEL_NAME=='lblQuery'">{{field.T_LANG2_TEXT}}</button>
