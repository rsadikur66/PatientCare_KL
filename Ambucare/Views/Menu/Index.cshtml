<body ng-app="Ambucare" ng-controller="menuController" ng-init="">

<div id="topbar">

    <div class="row">
        <div class="col-lg-10">
            <table style="width: 100%; border: none;">
                <tbody style="display:table; width:100%;">
                <tr style=" border: none; background: none;">
                    <td style="width: 30%; border: none; position: relative; top: -15px;" class="td-no-bg">
                        <img style="float: left; margin-left: 16px; margin-right: 10px;" src="@VirtualPathUtility.ToAbsolute("~/Images/user.png")" />
                        <div class="t-user-name">
                            <label style="font-size: 12px;" ID="lblUserId" class="ng-cloak">{{UserId}}</label><br />
                            <label style="font-size: 12px; width: 100%; margin-right: 13px;" ID="lblUserName" class="ng-cloak">{{UserName}}</label>
                        </div>
                        <label style="position: relative; top: -10px; border: none; font-size: 15px; color: white; cursor: pointer; margin-top: 5px;">
                            <img style="" src="@VirtualPathUtility.ToAbsolute("~/Images/arrow-down.png")" ng-click="onLogout()" />
                        </label>
                        <div class="t-panic">
                            <img src="~/Images/Panic.png" style="padding-left: 10px; height: 30px; width: 40px;"/>
                            <label style="position: relative; top: -9px;" ng-repeat="field in Menuentity track by $index" ng-if="field.T_LABEL_NAME=='lblPanicked'">{{field.T_LANG2_TEXT}}</label>
                        </div>
                    </td>
                    <td style="width: 40%; padding-left: 10px; border: none;" class="td-no-bg">
                        <div id="whiteLogo">
                            <img style="margin-top: 10px;" src="@VirtualPathUtility.ToAbsolute("~/Images/ambucareLogo5.png")" alt="white logo" />
                        </div>
                    </td>
                    <td style="width: 30%; border: none;position: relative; top: -20px;" class="td-no-bg">
                        <div class="t-calendar">
                            <img src="~/Images/calender.png"/>
                            <span style="position: relative; top: -8px;">
                                <script type="text/javascript">
                                    document.writeln(moment().format('DD/MM/YYYY'));
                                </script>
                            </span>
                                           
                                        
                        </div>
                        <div class="t-form-info" style="border-right: none;">
                            <label ID="lblFormInfo" class="ng-cloak" style="font-size: 13px; color: #5f6772; margin-right: 14px;">{{FName}}</label><br/>
                            <img src="~/Images/patientPad.png" style="width: 10px; height: 15px;"/>
                            <label ID="FName" class="ng-cloak">{{FCode}}</label>
                        </div>
                        <label style="display: none; background: bottom; border: none; font-size: 15px; color: white; cursor: pointer; margin-top: 5px;">
                            <img src="@VirtualPathUtility.ToAbsolute("~/Images/teamNotification.png")"/>
                        </label>
                        <label style="display: none;"> <span style="width: 100px; display: block; color: #364E6F; font-weight: 700;">Contact Team</span></label>
                    </td>
                </tr>
                </tbody>
            </table>
        </div>

    </div>
    <div id="divLogout" ng-hide="custom" class="logout_ltst">
        <div>
            <button Tooltip="Logout" class="Logout" ng-click="Logout()"></button>
            <label style="vertical-align: middle; cursor: pointer; position: relative; top: -7px;" ng-click="Logout()">Logout</label>
        </div>
    </div>
</div>

